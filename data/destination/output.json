{
  "title": "Jies test board",
  "description": "isf routes",
  "widgets": [
    {
      "id": "6748651480114544}",
      "definition": {
        "title": "Response Time (s)",
        "title_size": "16",
        "title_align": "left",
        "show_legend": true,
        "legend_layout": "auto",
        "legend_columns": [
          "avg",
          "min",
          "max",
          "value",
          "sum"
        ],
        "type": "timeseries",
        "requests": [
          {
            "formulas": [
              {
                "alias": "Response Time (s)",
                "style": {
                  "palette_index": 4,
                  "palette": "cool"
                },
                "formula": "query1 / 1000000000"
              }
            ],
            "queries": [
              {
                "data_source": "logs",
                "name": "query1",
                "indexes": [
                  "*"
                ],
                "compute": {
                  "aggregation": "avg",
                  "metric": "@duration"
                },
                "group_by": [],
                "search": {
                  "query": {
                    "query": "$kube-namespace $service @req.url:\\/query\\/bi\\/order-details/* @req.method:GET @req.statusCode:(2* OR 3*)"
                  }
                },
                "storage": "hot"
              }
            ],
            "response_format": "timeseries",
            "style": {
              "palette": "green",
              "line_type": "solid",
              "line_width": "normal"
            },
            "display_type": "line"
          }
        ],
        "yaxis": {
          "include_zero": false
        },
        "markers": []
      },
      "layout": {
        "x": 4,
        "y": 6,
        "width": 4,
        "height": 2
      }
    }
  ],
  "template_variables": [
    {
      "name": "kube-deployment",
      "prefix": "kube_deployment",
      "available_values": [
        "isf"
      ],
      "default": "isf"
    },
    {
      "name": "kube-service",
      "prefix": "kube_service",
      "available_values": [
        "isf"
      ],
      "default": "isf"
    },
    {
      "name": "kube-namespace",
      "prefix": "kube_namespace",
      "available_values": [
        "dc-production"
      ],
      "default": "dc-production"
    },
    {
      "name": "service",
      "prefix": "service",
      "available_values": [
        "isf"
      ],
      "default": "isf"
    },
    {
      "name": "ingress",
      "prefix": "@traefik.name",
      "available_values": [
        "dc-production-*"
